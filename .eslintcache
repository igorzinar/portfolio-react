idth: 100%;\r\n    height: 100vh;\r\n    object-fit: cover;\r\n  }\r\n`\r\n// Award Component\r\nconst Award = ({ title, description }) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title} </h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description} </p>\r\n    </AwardStyle>\r\n  )\r\n}\r\nconst Awards = styled.div`\r\n  min-height: 80vh;\r\n  display: flex;\r\n  margin: 5rem 10rem;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n`\r\nconst AwardStyle = styled.div`\r\n  padding: 5rem;\r\n  h3 {\r\n    font-size: 2rem;\r\n  }\r\n  .line {\r\n    width: 100%;\r\n    background: #23d997;\r\n    height: 0.5rem;\r\n    margin: 1rem 0rem;\r\n  }\r\n  p {\r\n    padding: 2rem 0rem;\r\n  }\r\n`\r\nexport default ProjectDetail\r\n","D:\\webprojects\\react\\react-portfolio\\src\\ProjectState.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\animation.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\components\\Wave.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\components\\Toggle.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\components\\useScroll.js",[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":21,"column":10,"nodeType":"89","messageId":"90","endLine":21,"endColumn":17},{"ruleId":"87","severity":1,"message":"91","line":21,"column":19,"nodeType":"89","messageId":"90","endLine":21,"endColumn":27},{"ruleId":"87","severity":1,"message":"92","line":22,"column":10,"nodeType":"89","messageId":"90","endLine":22,"endColumn":18},{"ruleId":"87","severity":1,"message":"93","line":22,"column":20,"nodeType":"89","messageId":"90","endLine":22,"endColumn":29},{"ruleId":"87","severity":1,"message":"94","line":13,"column":20,"nodeType":"89","messageId":"90","endLine":13,"endColumn":31},"no-native-reassign",["95"],"no-negated-in-lhs",["96"],"no-unused-vars","'element' is assigned a value but never used.","Identifier","unusedVar","'controls' is assigned a value 