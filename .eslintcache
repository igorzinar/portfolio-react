2"],"D:\\webprojects\\react\\react-portfolio\\src\\pages\\Projects.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\pages\\ProjectDetail.js",["83"],"import React, { useState, useEffect } from 'react'\r\nimport styled from 'styled-components'\r\nimport { useHistory } from 'react-router-dom'\r\nimport { ProjectState } from '../ProjectState'\r\n// Animations\r\nimport { motion } from 'framer-motion'\r\nimport { pageAnimation } from '../animation'\r\nconst ProjectDetail = () => {\r\n  const history = useHistory()\r\n  const url = history.location.pathname\r\n\r\n  // useState\r\n  const [projects, setProjects] = useState(ProjectState)\r\n  const [project, setProject] = useState(null)\r\n\r\n  // useEffect\r\n\r\n  useEffect(() => {\r\n    const currentProject = projects.filter(\r\n      (stateProject) => stateProject.url === url\r\n    )\r\n    setProject(currentProject[0])\r\n  }, [projects, url])\r\n\r\n  return (\r\n    <>\r\n      {project && (\r\n        <Details\r\n          exit=\"exit\"\r\n          variants={pageAnimation}\r\n          initial=\"hidden\"\r\n          animate=\"show\"\r\n        >\r\n          <HeadLine>\r\n            <h2>{project.title}</h2>\r\n            <img src={project.mainImg} alt=\"\" />\r\n          </HeadLine>\r\n          <Awards>\r\n            {project.awards.map((award) => (\r\n              <Award\r\n                title={award.title}\r\n                description={award.description}\r\n                key={award.title}\r\n              />\r\n            ))}\r\n          </Awards>\r\n          <ImageDisplay>\r\n            <img src={project.secondaryImg} alt=\"project\" />\r\n          </ImageDisplay>\r\n        </Details>\r\n      )}\r\n    </>\r\n  )\r\n}\r\n\r\nconst Details = styled(motion.div)`\r\n  color: #fff;\r\n`\r\nconst HeadLine = styled.div`\r\n  min-height: 90vh;\r\n  padding-top: 20vh;\r\n  position: relative;\r\n  h2 {\r\n    position: absolute;\r\n    top: 10%;\r\n    left: 50%;\r\n    transform: translate(-50%, -10%);\r\n  }\r\n  img {\r\n    width: 100%;\r\n    height: 70vh;\r\n    object-fit: cover;\r\n  }\r\n`\r\n\r\nconst ImageDisplay = styled.div`\r\n  min-height: 50vh;\r\n  img {\r\n    width: 100%;\r\n    height: 100vh;\r\n    object-fit: cover;\r\n  }\r\n`\r\n// Award Component\r\nconst Award = ({ title, description }) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title} </h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description} </p>\r\n    </AwardStyle>\r\n  )\r\n}\r\nconst Awards = styled.div`\r\n  min-height: 80vh;\r\n  display: flex;\r\n  margin: 5rem 10rem;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n`\r\nconst AwardStyle = styled.div`\r\n  padding: 5rem;\r\n  h3 {\r\n    font-size: 2rem;\r\n  }\r\n  .line {\r\n    width: 100%;\r\n    background: #23d997;\r\n    height: 0.5rem;\r\n    margin: 1rem 0rem;\r\n  }\r\n  p {\r\n    padding: 2rem 0rem;\r\n  }\r\n`\r\nexport default ProjectDetail\r\n","D:\\webprojects\\react\\react-portfolio\\src\\ProjectState.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\animation.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\components\\Wave.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\components\\Toggle.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\components\\useScroll.js",[],"D:\\webprojects\\react\\react-portfolio\\src\\components\\ScrollTop.js",[],{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","severity":1,"message":"89","line":74,"column":7,"nodeType":"90","messageId":"91","endLine":74,"endColumn":13},{"ruleId":"88","severity":1,"message":"92","line":13,"column":20,"nodeType":"90","messageId":"91","endLine":13,"endColumn":31},"no-native-reassign",["93"],"no-negated-in-lhs",["94"],"no-unused-vars","'Circle' is assigned a value but never used.","Identifier","unusedVar","'setProjects' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]